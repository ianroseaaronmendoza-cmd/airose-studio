{
  "name": "airose-studio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=prisma/schema.prisma --name init",
    "prisma:pull": "npx prisma db pull --schema=prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema=prisma/schema.prisma",
    "db:gen": "npm run prisma:generate",
    "db:sync": "npm run prisma:migrate",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "bcryptjs": "^2.4.3",
    "framer-motion": "^12.23.24",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "postcss": "^8.5.6",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
